{
    "name": "duncan3dc/sonos",
    "type": "library",
    "description": "Control Sonos speakers using a simple API",
    "keywords": ["sonos","upnp","streaming"],
    "homepage": "https://github.com/duncan3dc/sonos",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Craig Duncan",
        "email": "git@duncanc.co.uk",
        "homepage": "https://github.com/duncan3dc",
        "role": "Developer"
    }],
    "repositories": [
        {
            "type": "vcs",
            "url": "github.com:php-fig/cache"
        },
        {
            "type": "vcs",
            "url": "github.com:duncan3dc/cache"
        }
    ],
    "require": {
        "duncan3dc/cache": "dev-master",
        "duncan3dc/domparser": "^1.4.1",
        "duncan3dc/speaker": "^0.6.0",
        "guzzlehttp/guzzle": "^6.0",
        "league/flysystem": "^1.0",
        "psr/cache": "dev-master",
        "psr/log": "1.0.0",
        "ext-soap": "*",
        "ext-sockets": "*",
        "php": ">=7.0.0"
    },
    "require-dev": {
        "mockery/mockery": "1.0.x-dev",
        "phpunit/phpunit": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "duncan3dc\\Sonos\\": "src/",
            "duncan3dc\\SonosTests\\": "tests/"
        }
    }
}
